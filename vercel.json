{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "VITE_BACKEND_URL": "@VITE_BACKEND_URL"
  },
  "routes": [
    {
      "src": "/index.html",
      "status": 200,
      "continue": true
    },
    {
      "src": "/assets/.*",
      "status": 200,
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/.*",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
