{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(js|css|img|font)/(.+)",
      "dest": "/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
